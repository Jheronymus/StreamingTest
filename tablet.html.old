$('#social-stream').empty();
$('#social-stream').fadeOut('fast').load(
	$('#social-stream').dcSocialStream({
		feeds: {
			twitter: {
				id: 'fllbenelux,#fllbnl,@fllbenelux',
				thumb: true,
				images: 'small'
			},
			facebook: {
				id: '121516737994223'
			},
